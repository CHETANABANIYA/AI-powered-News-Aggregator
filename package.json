    "react-router-dom": "^7.1.3",
    "react-scripts": "^5.0.1",
    "serve": "^14.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "concurrently": "^8.2.1",
    "nodemon": "^3.1.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "node": ">=18 <=22"
  },
